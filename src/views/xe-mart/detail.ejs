                  <% if(i==0){ %>
                    <a class="nav-item nav-link active" data-toggle="tab" href="#sg<%= i+1%>"><img src="<%= image %>" alt=""></a>
                    <% } else{ %>
                      <a class="nav-item nav-link" data-toggle="tab" href="#sg<%= i+1%>"><img src="<%= image %>" alt=""></a>
