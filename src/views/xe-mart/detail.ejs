<!-- Breadcrumb Area -->
<section class="breadcrumb-area">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="breadcrumb-box text-center">
          <ul class="list-unstyled list-inline">
            <% products.breadcrumbs.forEach(function(breadcrumb) { %>
              <% if(breadcrumb.level!=0){ %>
                <li class="list-inline-item">
                  <a href="<%= breadcrumb.url %>">
                    <%= breadcrumb.name %>
                  </a> <span>||</span></li>
                <% } else{ %>
                  <li class="list-inline-item">
                    <%= breadcrumb.name %>
                  </li>
                  <% } %>
                    <% }); %>
          </ul>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- End Breadcrumb Area -->

