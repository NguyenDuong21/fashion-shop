                <% products.img.forEach(function(image, i) { %>
                  <% if(i==0){ %>
                    <div class="tab-pane fade show active" id="sg<%= i+1%>" role="tabpanel">
                      <img src="<%= image %>" alt="" class="img-fluid">
                    </div>
                    <% } else{ %>
                      <div class="tab-pane" id="sg<%= i+1%>" role="tabpanel">
